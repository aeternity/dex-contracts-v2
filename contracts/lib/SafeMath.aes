@compiler >= 6

namespace SafeMath =
    
    function add(x: int, y: int): int =
        /*require(x >= 0 && y >= 0,"LP")*/
        x + y

    function sub(x: int, y: int): int =
        /*require(x >= 0 && y >= 0,"LP")*/
        x - y

    function mul(x: int, y: int): int =
        /*require(x >= 0 && y >= 0,"LP")*/
        x * y
